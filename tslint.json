{
  "extends": "tslint:recommended",
  "linterOptions": {
    "exclude": [
      "node_modules"
    ]
  },
  "rules": {
    "no-empty-interface": false,
    "no-console": false,
    "trailing-comma": [
      true, {
        "multiline": "always",
        "singleline": "never"
      }
    ],
    "no-consecutive-blank-lines": true,
    "quotemark": [true, "single"],
    "arrow-parens": false
  }
}